}
.porcelain-title {
  font-size: 30rpx;
  color: #444;
  margin-bottom: 20rpx;
  line-height: 30rpx;
}
.no-wrap {
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}
.m-no-wrap {
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
